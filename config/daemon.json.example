{
    "exec-opts": ["native.cgroupdriver=systemd"],
    "log-driver": "json-file",
    "log-level": "",
    "log-opts": {
      "cache-disabled": "false",
      "cache-max-file": "25",
      "cache-max-size": "250m",
      "cache-compress": "true",
      "env": "os,customer",
      "labels": "k8s-master-node",
      "max-file": "5",
      "max-size": "250m"
    },
    "storage-driver": "overlay2",
    "storage-opts": [
      "overlay2.override_kernel_check=true"
    ],
    "selinux-enabled": false,
    "allow-nondistributable-artifacts": [],
    "api-cors-header": "",
    "authorization-plugins": [],
    "bip": "",
    "bridge": "",
    "cgroup-parent": "",
    "cluster-advertise": "",
    "cluster-store": "",
    "cluster-store-opts": {},
    "containerd": "/run/containerd/containerd.sock",
    "containerd-namespace": "docker",
    "containerd-plugin-namespace": "docker-plugins",
    "data-root": "",
    "debug": true,
    "default-address-pools": [
      {
        "base": "172.30.0.0/16",
        "size": 24
      },
      {
        "base": "172.31.0.0/16",
        "size": 24
      }
    ],
    "default-cgroupns-mode": "private",
    "default-gateway": "",
    "default-gateway-v6": "",
    "default-runtime": "runc",
    "default-shm-size": "64M",
    "default-ulimits": {
      "nofile": {
        "Hard": 64000,
        "Name": "nofile",
        "Soft": 64000
      }
    },
    "dns": [],
    "dns-opts": [],
    "dns-search": [],
    "exec-root": "",
    "experimental": false,
    "features": {},
    "fixed-cidr": "",
    "fixed-cidr-v6": "",
    "group": "",
    "hosts": [],
    "icc": false,
    "init": false,
    "init-path": "/usr/libexec/docker-init",
    "insecure-registries": [],
    "ip": "0.0.0.0",
    "ip-forward": false,
    "ip-masq": false,
    "iptables": false,
    "ip6tables": false,
    "ipv6": false,
    "labels": [],
    "live-restore": true,
    "max-concurrent-downloads": 3,
    "max-concurrent-uploads": 5,
    "max-download-attempts": 5,
    "mtu": 0,
    "no-new-privileges": false,
    "node-generic-resources": [
      "NVIDIA-GPU=UUID1",
      "NVIDIA-GPU=UUID2"
    ],
    "oom-score-adjust": -500,
    "pidfile": "",
    "raw-logs": false,
    "registry-mirrors": [],
    "runtimes": {
      "cc-runtime": {
        "path": "/usr/bin/cc-runtime"
      },
      "custom": {
        "path": "/usr/local/bin/my-runc-replacement",
        "runtimeArgs": [
          "--debug"
        ]
      }
    },
    "seccomp-profile": "",
    "shutdown-timeout": 15,
    "swarm-default-advertise-addr": "",
    "tls": true,
    "tlscacert": "",
    "tlscert": "",
    "tlskey": "",
    "tlsverify": true,
    "userland-proxy": false,
    "userland-proxy-path": "/usr/libexec/docker-proxy",
    "userns-remap": "" 
